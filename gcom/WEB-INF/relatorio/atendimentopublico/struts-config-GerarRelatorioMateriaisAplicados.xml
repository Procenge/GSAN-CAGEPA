<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>


	<form-beans>
		<form-bean
			name="GerarRelatorioMateriaisAplicadosActionForm"
			type="gcom.gui.relatorio.atendimentopublico.GerarRelatorioMateriaisAplicadosActionForm" />
	</form-beans>

	<!--******************* PAGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action
			name="GerarRelatorioMateriaisAplicadosActionForm"
			type="gcom.gui.relatorio.atendimentopublico.ExibirGerarRelatorioMateriaisAplicadosAction"
			validate="true"
			path="/exibirGerarRelatorioMateriaisAplicadosAction">

			<forward
				name="exibirGerarRelatorioMateriaisAplicados"
				path="/jsp/relatorio/atendimentopublico/relatorio_materiais_aplicados_gerar.jsp" />

		</action>

		<action
			name="GerarRelatorioMateriaisAplicadosActionForm"
			type="gcom.gui.relatorio.cadastro.imovel.GerarRelatorioMateriaisAplicadosAction"
			validate="true"
			path="/gerarRelatorioMateriaisAplicadosAction">

			<forward
				name="exibirGerarRelatorioMateriaisAplicados"
				path="/jsp/relatorio/atendimentopublico/relatorio_materiais_aplicados_gerar.jsp" />
		</action>


	</action-mappings>

</struts-config>