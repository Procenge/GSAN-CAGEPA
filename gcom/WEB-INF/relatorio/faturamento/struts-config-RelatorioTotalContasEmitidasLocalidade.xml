<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="RelatorioTotalContasEmitidasLocalidadeForm"
			type="gcom.gui.relatorio.faturamento.RelatorioTotalContasEmitidasLocalidadeForm" />

	</form-beans>

	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />

	<action-mappings>
		<!--******************* PÁGINA PRINCIPAL *******************-->
		<action
			name="RelatorioTotalContasEmitidasLocalidadeForm"
			type="gcom.gui.relatorio.faturamento.ExibirFiltroRelatorioTotalContasEmitidasLocalidadeAction"
			input="/exibirFiltroRelatorioTotalContasEmitidasLocalidade.do"
			path="/exibirFiltroRelatorioTotalContasEmitidasLocalidade">
			
			<forward name="exibirFiltroRelatorioTotalContasEmitidasLocalidade" path="/jsp/relatorio/faturamento/relatorio_total_contas_emitidas_localidade.jsp"></forward>
		</action>
		
		<action
			name="RelatorioTotalContasEmitidasLocalidadeForm"
			type="gcom.gui.relatorio.faturamento.GerarRelatorioTotalContasEmitidasLocalidadeAction"
			input="/gerarRelatorioTotalContasEmitidasLocalidade.do"
			path="/gerarRelatorioTotalContasEmitidasLocalidade">
			
			<forward name="gerarRelatorioTotalContasEmitidasLocalidade" path="/jsp/relatorio/faturamento/relatorio_total_contas_emitidas_localidade.jsp"></forward>
		</action>
		
		
	</action-mappings>


</struts-config>