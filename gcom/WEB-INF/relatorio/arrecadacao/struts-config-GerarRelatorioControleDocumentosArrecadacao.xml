	</form-beans>

	<!--******************* P?GINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
		
		<action name="FiltrarRelatorioControleDocumentosArrecadacaoActionForm"
			    type="gcom.gui.relatorio.arrecadacao.ExibirGerarRelatorioControleDocumentosArrecadacaoAction"
			    validate="true" 
			    path="/exibirGerarRelatorioControleDocumentosArrecadacaoAction">
			<forward name="exibirGerarRelatorioControleDocumentosArrecadacao" path="/jsp/relatorio/arrecadacao/relatorio_controle_documentos_arrecadacao.jsp" />
		</action>

		<action name="FiltrarRelatorioControleDocumentosArrecadacaoActionForm"
				type="gcom.gui.relatorio.arrecadacao.GerarRelatorioControleDocumentosArrecadacaoAction"
				validate="false" 
				path="/gerarRelatorioControleDocumentosArrecadacaoAction">			
		</action>				
		
	</action-mappings>

	
</struts-config>