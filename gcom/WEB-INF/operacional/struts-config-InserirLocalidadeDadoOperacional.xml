<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="InserirLocalidadeDadoOperacionalActionForm"
			type="gcom.gui.operacional.localidadedadooperacional.InserirLocalidadeDadoOperacionalActionForm" />
	</form-beans>
	<global-forwards />
	<action-mappings>

		<action name="InserirLocalidadeDadoOperacionalActionForm"
				type="gcom.gui.operacional.localidadedadooperacional.ExibirInserirLocalidadeDadoOperacionalAction"
				validate="true" 
				scope="session"
				path="/exibirInserirLocalidadeDadoOperacionalAction">
			<forward name="inserirLocalidadeDadoOperacional"	path="/jsp/operacional/localidadedadooperacional/localidadedadooperacional_inserir.jsp" />
		</action>
		<action name="InserirLocalidadeDadoOperacionalActionForm"
				type="gcom.gui.operacional.localidadedadooperacional.InserirLocalidadeDadoOperacionalAction"
				validate="true" 
				scope="session"
				path="/inserirLocalidadeDadoOperacionalAction">
		</action>

	</action-mappings>
	<message-resources null="false" parameter="gcom.properties.application" />
</struts-config>