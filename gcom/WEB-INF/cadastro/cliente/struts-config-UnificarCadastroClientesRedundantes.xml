<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="UnificarCadastroClientesRedundantesActionForm"
			type="gcom.gui.cadastro.cliente.UnificarCadastroClientesRedundantesActionForm" />			
			
	</form-beans>

	<!--******************* PÁGINAS GLOBAIS *******************-->
	<global-forwards />

	<action-mappings>
		<!--******************* PÁGINA PRINCIPAL *******************-->
		<!-- [UC0472] CONSULTAR IMOVEL Auhtor: Rafael Santos Data:07/09/2006-->
		<action name="UnificarCadastroClientesRedundantesActionForm"
			type="gcom.gui.cadastro.cliente.ExibirUnificarCadastroClientesRedundantesAction"
			validate="false" input="/exibirUnificarCadastroClientesRedundantesAction.do"
			scope="session" path="/exibirUnificarCadastroClientesRedundantesAction">
			<forward name="exibirUnificarCadastroClientesRedundantesAction"
				path="/jsp/cadastro/cliente/unificacao_cadastro_clientes_redundantes.jsp" />
		</action>
		
	</action-mappings>

</struts-config>