<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="ConsultarHistoricoMovimentoDebitoAutomaticoFiltrarActionForm"
			type="gcom.gui.arrecadacao.debitoautomatico.ConsultarHistoricoMovimentoDebitoAutomaticoFiltrarActionForm" />


	</form-beans>

	<!--******************* PÃGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action name="ConsultarHistoricoMovimentoDebitoAutomaticoFiltrarActionForm"
			type="gcom.gui.arrecadacao.debitoautomatico.ConsultarHistoricoMovimentoDebitoAutomaticoFiltrarAction"
			validate="false" 
			path="/consultarHistoricoMovimentoDebitoAutomaticoFiltrarAction">
			<forward name="consultarHistoricoMovimentoDebitoAutomaticoFiltrar"
				path="/jsp/arrecadacao/debitoautomatico/consultar_historico_movimento_debito_automatico_filtrar.jsp" />
		</action>

	</action-mappings>
	<message-resources null="false"
		parameter="gcom.properties.application" />
</struts-config>