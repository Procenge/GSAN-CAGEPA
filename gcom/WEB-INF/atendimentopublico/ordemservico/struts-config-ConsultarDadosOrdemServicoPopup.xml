<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean 
		    name="ConsultarDadosOrdemServicoPopupActionForm"
			type="gcom.gui.atendimentopublico.ordemservico.ConsultarDadosOrdemServicoPopupActionForm" />
			
		<form-bean 
		    name="ConsultarDadosAtendimentoPopupActionForm"
			type="gcom.gui.cadastro.atendimento.ConsultarDadosAtendimentoPopupActionForm" />			
			 
	</form-beans>

	<!--******************* PÃGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>
				
		<action name="ConsultarDadosOrdemServicoPopupActionForm"  
			type="gcom.gui.atendimentopublico.ordemservico.ExibirConsultarDadosOrdemServicoPopupAction" 
			validate="true"
			scope="session"
			path="/exibirConsultarDadosOrdemServicoPopupAction">

			<forward name="consultarDadosOrdemServicoPopup"
			path="/jsp/atendimentopublico/ordemservico/ordem_servico_dados_consultar_popup.jsp" />
		</action>
		
		<action name="ConsultarDadosAtendimentoPopupActionForm"  
			type="gcom.gui.cadastro.atendimento.ExibirConsultarDadosAtendimentoPopupAction" 
			validate="true"
			scope="session"
			path="/exibirConsultarDadosAtendimentoPopupAction">

			<forward name="consultarDadosAtendimentoPopup"
			path="/jsp/cadastro/atendimento/atendimento_dados_consultar_popup.jsp" />
		</action>		
		
	</action-mappings>

	<message-resources null="false"
		parameter="gcom.properties.application" />
		
</struts-config>