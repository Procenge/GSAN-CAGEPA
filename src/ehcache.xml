     maxElementsInMemory="30"
     eternal="false"
     timeToIdleSeconds="300"
     timeToLiveSeconds="300"
     overflowToDisk="true"
 />
 
<cache
     name="gcom.cobranca.NacionalFeriado"
     maxElementsInMemory="30"
     eternal="false"
     timeToIdleSeconds="300"
     timeToLiveSeconds="300"
     overflowToDisk="true"
 />
 
<cache
     name="gcom.cadastro.geografico.Bairro"
     maxElementsInMemory="450"
     eternal="false"
     timeToIdleSeconds="300"
