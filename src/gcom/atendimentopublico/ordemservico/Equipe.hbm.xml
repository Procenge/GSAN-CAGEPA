<class 
    name="gcom.atendimentopublico.ordemservico.Equipe" 
    table="equipe"
>

    <id
        name="id"
        type="java.lang.Integer"
        column="eqpe_id"
    >
        <generator class="sequence">
       		<param name="sequence">sq_equipe</param>
    	</generator>
    </id>

    <property
        name="nome"
        type="java.lang.String"
        column="eqpe_nmequipe"
        not-null="true"
        length="20"
    />
    <property
        name="placaVeiculo"
        type="java.lang.String"
        column="eqpe_nnplacaveiculo"
        length="8"
    />
    <property
        name="cargaTrabalho"
        type="java.lang.Integer"
        column="eqpe_nncargatrabalhodia"
        length="4"
    />
    <property
        name="indicadorUso"
        type="short"
        column="eqpe_icuso"
        not-null="true"
        length="2"
    />
    <property
        name="ultimaAlteracao"
        type="java.sql.Timestamp"
        column="eqpe_tmultimaalteracao"
        not-null="true"
        length="8"
    />
    
    <!-- bi-directional many-to-one association to UnidadeOrganizaci onal -->
    <many-to-one
        name="unidadeOrganizacional"
        class="gcom.cadastro.unidade.UnidadeOrganizacional"
        not-null="true"
    >
        <column name="unid_id" />
    </many-to-one>
    
    <many-to-one
        name="equipeTipo"
        class="gcom.atendimentopublico.ordemservico.EquipeTipo"        
    >
        <column name="eqtp_id" />
    </many-to-one>
    
    <!-- bi-directional many-to-one association to ServicoPerfilTipo -->
    <many-to-one
        name="servicoPerfilTipo"
        class="gcom.atendimentopublico.ordemservico.ServicoPerfilTipo"
        not-null="true"
    >
        <column name="sptp_id" />
    </many-to-one>

