      </generator>

    </id>
    
    <property
        name="descricao"
        type="java.lang.String"
        column="cbfm_dscobrancaforma"
        length="40"
    />
    <property
        name="descricaoAbreviada"
        type="java.lang.String"
        column="cbfm_dsabreviado"
        length="20"
    />
    <property
        name="indicadorUso"
        type="java.lang.Short"
        column="cbfm_icuso"
        length="2"
    />
    <property
        name="ultimaAlteracao"
        type="java.sql.Timestamp"
        column="cbfm_tmultimaalteracao"
        length="8"
    />

