<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="gcom.cobranca.QuitacaoDebitoAnual" 
    table="quitacao_debito_anual"
>

    <id
        name="id"
        type="java.lang.Integer"
        column="qadb_id"
    >
        <generator class="sequence">
			<param name="sequence">
				sq_quitacao_debito_anual
			</param>
		</generator>
    </id>

    <property
        name="anoReferencia"
        type="java.lang.Integer"
        column="qadb_aareferencia"
        length="4"
        not-null="true"
    />
    <property
        name="indicadorImpressao"
        type="java.lang.Short"
        column="qadb_icimpressao"
        length="2"
        not-null="true"
    />
    <property
        name="ultimaAlteracao"
        type="java.sql.Timestamp"
        column="qadb_tmultimaalteracao"
        length="8"
        not-null="true"
    />
   
 <property
        name="anoReferenciaGeracao"
        type="java.lang.Integer"
        column="QADB_AMREFERENCIAGERACAO"
        length="4"
        not-null="true"
    />
   
   
    <!-- Associations -->
    
    <!-- bi-directional many-to-one association to Imovel -->
    <many-to-one
        name="imovel"
        class="gcom.cadastro.imovel.Imovel"
        not-null="true"
    >
    	<column name="imov_id" />
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Cliente -->
    <many-to-one
        name="clienteResponsavel"
        class="gcom.cadastro.cliente.Cliente"
        not-null="false"
    >
        <column name="clie_idresponsavel" />
    </many-to-one> 
    
    <!-- bi-directional many-to-one association to Cliente -->
    <many-to-one
        name="clienteUsuario"
        class="gcom.cadastro.cliente.Cliente"
        not-null="false"
    >
        <column name="clie_idusuario" />
    </many-to-one> 
    
    <!-- bi-directional many-to-one association to FaturamentoGrupo -->
    <many-to-one
        name="faturamentoGrupo"
        class="gcom.faturamento.FaturamentoGrupo"
        not-null="false"
    >
        <column name="ftgr_id" />
    </many-to-one>
    

  
 </class>
</hibernate-mapping>