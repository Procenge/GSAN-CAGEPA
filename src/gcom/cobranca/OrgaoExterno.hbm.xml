<class 
    name="gcom.cobranca.OrgaoExterno" 
    table="orgao_externo"
>

    
    <id 
        name="id" 
        column="orga_id" 
        type="java.lang.Integer"
        length="4" >
      <generator class="sequence">
          <param name="sequence">sq_orgao_externo</param>
      </generator>
     
     </id>
      
	<property
        name="descricao"
        type="java.lang.String"
        column="orga_dsorgaoexterno"
        length="45"
    />
    
    <property
        name="indicadorUso"
        type="java.lang.Integer"
        column="orga_icuso"
        length="8"
    />
    
	<property
        name="ultimaAlteracao"
        type="java.sql.Timestamp"
        column="orga_tmultimaalteracao"
        length="8"
    />

  

</class>
</hibernate-mapping>