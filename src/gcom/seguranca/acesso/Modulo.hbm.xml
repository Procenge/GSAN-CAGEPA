<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<!-- 
		Created by the Middlegen Hibernate plugin 2.1
		
		http://boss.bekk.no/boss/middlegen/
		http://www.hibernate.org/
	-->

	<class name="gcom.seguranca.acesso.Modulo" table="modulo">

		<id name="id" type="java.lang.Integer" column="modu_id">
			<generator class="sequence">
				<param name="sequence">
					sq_modulo
				</param>
			</generator>
		</id>

		<property 
			name="descricaoModulo" 
			type="java.lang.String"
			column="modu_dsmodulo" 
			length="60" 
		/>
		<property 
			name="descricaoAbreviado" 
			type="java.lang.String"
			column="modu_dsabreviado" 
			length="10" 
		/>
		<property 
			name="ultimaAlteracao" 
			type="java.sql.Timestamp"
			column="modu_tmultimaalteracao" 
			length="8" 
		/>

		<!-- Associations -->

		<!-- bi-directional one-to-many association to Funcionalidade -->
		<!-- 
			<set
			name="funcionalidades"
			lazy="true"
			inverse="true"
			cascade="all"
			>
			<key>
			<column name="modu_id" />
			</key>
			<one-to-many 
			class="gcom.Funcionalidade"
			/>
			</set>
		-->
	</class>
</hibernate-mapping>
