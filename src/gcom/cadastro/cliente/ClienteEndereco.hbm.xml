<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<!--
    Created by the Middlegen Hibernate plugin

    http://boss.bekk.no/boss/middlegen/
    http://hibernate.sourceforge.net/
-->

<class
    table="cliente_endereco"
    name="gcom.cadastro.cliente.ClienteEndereco"
>

    <id
        name="id"
        type="java.lang.Integer"
        column="cled_id"
    >

    <generator class="sequence">
        <param name="sequence">sq_cliente_end</param>
    </generator>

    </id>

    <property
        name="numero"
        type="java.lang.String"
        column="cled_nnimovel"
        length="5"
    />
    <property
        name="complemento"
        type="java.lang.String"
        column="cled_dscomplementoendereco"
        length="25"
    />
    <property
        name="indicadorEnderecoCorrespondencia"
        type="java.lang.Short"
        column="cled_icenderecocorrespondencia"
        length="2"
    />
    <property
        name="ultimaAlteracao"
        type="java.sql.Timestamp"
        column="cled_tmultimaalteracao"
        length="8"
    />
    
    <property
        name="logradouro"
        type="java.lang.Integer"
        column="logr_id"
        length="4"
    />

    <!-- associations -->
    <!-- bi-directional many-to-one association to EnderecoTipo -->
    <many-to-one
        name="enderecoTipo"
        class="gcom.cadastro.endereco.EnderecoTipo"
        not-null="true"
    >
        <column name="edtp_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Cliente -->
    <many-to-one
        name="cliente"
        class="gcom.cadastro.cliente.Cliente"
        not-null="true"
    >
        <column name="clie_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to EnderecoReferencia -->
    <many-to-one
        name="enderecoReferencia"
        class="gcom.cadastro.endereco.EnderecoReferencia"
        not-null="true"
    >
        <column name="edrf_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to LogradouroBairro -->
    <many-to-one
        name="logradouroBairro"
        class="gcom.cadastro.endereco.LogradouroBairro"
        not-null="false"
    >
        <column name="lgbr_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to LogradouroCep -->
    <many-to-one
        name="logradouroCep"
        class="gcom.cadastro.endereco.LogradouroCep"
        not-null="false"
    >
        <column name="lgcp_id" />
    </many-to-one>
    
    <!-- bi-directional many-to-one association to Imovel -->
    <many-to-one
        name="imovel"
        class="gcom.cadastro.imovel.Imovel"
        not-null="true"
    >
        <column name="imov_id" />
    </many-to-one>

</class>
</hibernate-mapping>
