        inverse="true"
	cascade="delete"
    >
        <key>
            <column name="clie_id" />
        </key>
        <one-to-many
            class="gcom.cadastro.cliente.ClienteFone"
        />
    </set>
    <!-- bi-directional one-to-many association to ClienteImovel -->
