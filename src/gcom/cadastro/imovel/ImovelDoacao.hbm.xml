<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="gcom.cadastro.imovel.ImovelDoacao" 
    table="imovel_doacoes"
>

    <id
        name="id"
        type="java.lang.Integer"
        column="imdo_id"
    >
      <generator class="sequence">
          <param name="sequence">sq_imov_doacoes</param>
      </generator>
    </id>

    <property
        name="valorDoacao"
        type="java.math.BigDecimal"
        column="imdo_vldoacao"
        not-null="true"
        length="8"
    />
    <property
        name="dataAdesao"
        type="java.sql.Date"
        column="imdo_dtadesao"
        not-null="true"
        length="4"
    />
    <property
        name="dataCancelamento"
        type="java.sql.Date"
        column="imdo_dtcancelamento"
        length="4"
    />
    <property
        name="ultimaAlteracao"
        type="java.sql.Timestamp"
        column="imdo_tmultimaalteracao"
        not-null="true"
        length="8"
    />
    
	<property
        name="referenciaInicial"
        type="java.lang.Integer"
        column="imdo_amreferenciainicial"
        length="4"
    />
    
	<property
        name="referenciaFinal"
        type="java.lang.Integer"
        column="imdo_amreferenciafinal"
        length="4"
    />        

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to EntidadeBeneficenteContrato -->
    <many-to-one
        name="entidadeBeneficenteContrato"
        class="gcom.cadastro.imovel.EntidadeBeneficenteContrato"
    >
        <column name="ebct_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Usuario -->
    <many-to-one
        name="usuarioAdesao"
        class="gcom.seguranca.acesso.usuario.Usuario"
        not-null="true"
    >
        <column name="usur_idadesao" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Usuario -->
    <many-to-one
        name="usuarioCancelamento"
        class="gcom.seguranca.acesso.usuario.Usuario"
        not-null="true"
    >
        <column name="usur_idcancelamento" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Imovel -->
    <many-to-one
        name="imovel"
        class="gcom.cadastro.imovel.Imovel"
        not-null="true"
    >
        <column name="imov_id" />
    </many-to-one>

</class>
</hibernate-mapping>
