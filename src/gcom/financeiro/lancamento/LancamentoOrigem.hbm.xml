<class 
	name="gcom.financeiro.lancamento.LancamentoOrigem" 
    table="lancamento_origem"
>
	<id
        name="id"
        type="java.lang.Integer"
        column="lcor_id"
    >
      <generator class="sequence">
          <param name="sequence">sq_lancamento_origem</param>
      </generator>

    </id>
    
    <property
        name="descricao"
        type="java.lang.String"
        column="lcor_dslancamentoorigem"
        length="30"
    />
    <property
        name="descricaoAbreviada"
        type="java.lang.String"
        column="lcor_dsabreviado"
        length="10"
    />
    <property
        name="ultimaAlteracao"
        type="java.sql.Timestamp"
        column="lcor_tmultimaalteracao"
        length="8"
    />
    <property
        name="numeroCartao"
        type="java.lang.Short"
        column="lcor_nncartao"
        not-null="true"
        length="2"
    />
    <property
        name="codigoTipo"
        type="java.lang.Short"
        column="lcor_cdtipo"
        not-null="true"
        length="2"
    />
    <property
        name="numeroFolha"
        type="java.lang.Short"
        column="lcor_nnfolha"
        not-null="true"
        length="2"
    />
    <property
        name="codigoReferencia"
        type="java.lang.String"
        column="lcor_cdreferencia"
        not-null="true"
        length="3"
    />
    <property
        name="numeroVersao"
        type="java.lang.Short"
        column="lcor_nnversao"
        not-null="true"
        length="2"
    />
    <property
        name="numeroCartao2"
        type="java.lang.Short"
        column="lcor_nncartao2"
        not-null="true"
        length="2"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to LancamentoContabil -->
