<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="gcom.financeiro.lancamento.AjusteContabilidade"
		table="ajuste_contabilidade">

		<id name="id" type="java.lang.Integer" column="id">
		</id>

		<property name="idPagamento" type="java.lang.Integer" column="pghi_id" />

		<property name="icProcessado" type="java.lang.Integer"
			column="icprocessado" />

	</class>
</hibernate-mapping>
