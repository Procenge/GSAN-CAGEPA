<class
    name="gcom.faturamento.consumotarifa.ConsumoTarifa"
    table="consumo_tarifa"
>

    <id
        name="id"
        type="java.lang.Integer"
        column="cstf_id"
    >
        <generator class="sequence">
          <param name="sequence">sq_consumo_tarifa</param>
      	</generator>
    </id>

    <property
        name="descricao"
        type="java.lang.String"
        column="cstf_dsconsumotarifa"
        length="30"
    />
    <property
        name="indicadorUso"
        type="java.lang.Short"
        column="cstf_icuso"
        length="2"
    />
    <property
        name="ultimaAlteracao"
        type="java.sql.Timestamp"
        column="cstf_tmultimaalteracao"
        length="8"
    />
    <property
        name="icTarifaEsgotoPropria"
        type="java.lang.Short"
        column="cstf_ictarifaesgotopropria"
        length="2"
    />

    <!-- associations -->
    <!-- bi-directional one-to-many association to ConsumoTarifaVigencia -->
    
   <set
     name="consumoTarifaVigencias"
     lazy="true"
     inverse="true"
   >
     <key>
         <column name="cstf_id" />
     </key>
     <one-to-many
         class="gcom.faturamento.consumotarifa.ConsumoTarifaVigencia"
     />
    </set>
